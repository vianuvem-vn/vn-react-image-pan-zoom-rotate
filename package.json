{
  "name": "vn-react-image-pan-zoom-rotate",
  "version": "1.2.4",
  "description": "Simple wrapper for pan, zoom, rotate on react containers",
  "main": "dist/PanViewer",
  "typings": "dist/PanViewer.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vianuvem-vn/vn-react-image-pan-zoom-rotate"
  },
  "keywords": [
    "html pan",
    "html zoom",
    "react pan zoom",
    "react pan zoom rotate"
  ],
  "scripts": {
    "start:dev": "webpack-dev-server --host 0.0.0.0 --port 8888",
    "lib:build": "webpack --config=webpack.config.lib.js",
    "test": "jest --watchAll --coverage",
    "itest": "cypress open",
    "prepublishOnly": "npm run lib:build"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.19",
    "cypress": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^3.2.6",
    "svg-sprite-loader": "^3.7.3",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "4.19.0",
    "webpack-cli": "3.0.4",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "coverageReporters": [
      "html"
    ]
  }
}